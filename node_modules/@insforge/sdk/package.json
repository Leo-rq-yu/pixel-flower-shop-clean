{
  "name": "@insforge/sdk",
  "version": "0.0.54",
  "description": "TypeScript SDK for InsForge Backend-as-a-Service platform",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:tsc": "tsc && tsc --module commonjs --outDir dist/cjs",
    "build:tsc-only": "tsc",
    "dev": "tsup --watch",
    "dev:tsc": "tsc --watch",
    "test": "vitest",
    "test:integration": "node integration-tests/run-all.js",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "insforge",
    "baas",
    "backend",
    "sdk",
    "typescript"
  ],
  "author": "InsForge",
  "license": "MIT",
  "dependencies": {
    "@insforge/shared-schemas": "^1.0.7",
    "@supabase/postgrest-js": "^1.21.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=18"
  }
}
